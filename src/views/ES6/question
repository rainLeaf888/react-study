// 输出顺序
window.addEventListener('click', () => {
  new Promise((resolve, reject) => {
    console.log(1)
    resolve();
  }).then(() => {
    console.log(2)
  })

}, false)
window.addEventListener('click', () => {
  new Promise((resolve, reject) => {
    console.log(3)
    resolve();
  }).then(() => {
    console.log(4)
  })
}, false)
