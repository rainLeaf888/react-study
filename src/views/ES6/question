function TreeNode(value, left, right) {
    this.value = value;
    this.left = left;
    this.right = right;
}

var x = new TreeNode(4);
var y = new TreeNode(5);
var z = new TreeNode(6);
var left = new TreeNode(2, x, y);
var right = new TreeNode(3, z);
var root = new TreeNode(1, left, right);

